{
  "name": "vitepress-plugin-folder-tree",
  "version": "1.1.0",
  "description": "Interactive, collapsible file/folder tree diagrams for VitePress â€” supports YAML, ASCII, external URLs, filesystem scan, tabs, search, git status badges, deep linking, and more",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "vue-tsc --noEmit",
    "build": "vue-tsc --noEmit && vite build",
    "dev:watch": "vite build --watch",
    "dev:docs": "pnpm -C example docs:dev",
    "dev": "pnpm build && concurrently -k -n lib,docs -c blue,green \"pnpm dev:watch\" \"pnpm dev:docs\"",
    "docs:build": "pnpm build && pnpm -C example docs:build",
    "docs:preview": "pnpm -C example docs:preview"
  },
  "dependencies": {
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "concurrently": "^9.2.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.2.0",
    "vitepress": "^1.0.0",
    "vue": "^3.5.0",
    "vue-tsc": "^3.2.4",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "vitepress": "^1.0.0"
  },
  "keywords": [
    "vitepress",
    "vitepress-plugin",
    "folder-tree",
    "file-tree",
    "tree",
    "directory-tree",
    "directory-structure",
    "markdown",
    "markdown-it",
    "vue",
    "vue3",
    "collapsible",
    "interactive",
    "ascii-tree",
    "yaml",
    "code-block",
    "docs",
    "documentation"
  ],
  "author": "s00d",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/s00d/vitepress-plugin-folder-tree.git"
  },
  "homepage": "https://github.com/s00d/vitepress-plugin-folder-tree",
  "bugs": {
    "url": "https://github.com/s00d/vitepress-plugin-folder-tree/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://www.donationalerts.com/r/s00d88"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": [
    "**/*.vue",
    "**/*.css"
  ]
}
